<body>
  <div id="outer">
    <div id="inner">
      <div id="header">
        {{{header i18n/aboutServer this}}}
      </div>

      <div id="body">

        <div class="uk-container uk-container-small">
          <h4 class="uk-text-bold">SunLight Project</h4>
          <p>
            Branch: {{serverInfo/branch}} <br>
            Commit: {{serverInfo/commit}} <br>
            Date: {{serverInfo/commitDate}} <br>
            Client Version: {{serverInfo/clientVersion}} <br>
            Bundle Version: {{serverInfo/bundleVersion}} <br>
            {{i18n/currentOnline}}: {{serverInfo/currentOnline}} <br>
            Запущен {{serverInfo/uptime}} <br>
            {{#if serverInfo/supportMail}}
            {{serverInfo/supportMail}}
            {{/if}}
          </p>
        </div>
      </div>

      {{{changeLanguageModal}}}
    </div>
  </div>

  <script type="text/javascript">
    var userId = "{{{ userId }}}"
    var authToken = "{{{ authToken }}}"
    var external = {{{ external }}}
    var headers = {{{ headers }}}
  </script>
  <script type="text/javascript" src="/resources/js/uikit.min.js"></script>
  <script type="text/javascript" src="/resources/js/jquery.min.js"></script>
  <script type="text/javascript" src="/resources/js/common.js"></script>
  <script type="text/javascript" src="/resources/js/uikit-icons.min.js"></script>
  <script type="text/javascript" src="/resources/js/smooth-scrollbar.js"></script>

  <script>
    var scrollbar = Scrollbar.init($("#body")[0])
  </script>
</body>